<header>
<ul>
          {% for link in section.settings.menu.links %}
            <li>
              <a href="{{ link.url }}">
                <span>{{ link.title }}</span>
              </a>
            </li>
          {% endfor %}
        </ul>
</header>

{% schema %}
  {
    "name": "Header",
    "settings": [
      {
        "type": "link_list",
        "id": "menu",
        "default": "main-menu",
        "label": "t:sections.header.settings.menu.label"
      }, 
    ]
  }
{% endschema %}